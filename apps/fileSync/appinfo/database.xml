<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

  <name>*dbname*</name>
  <create>true</create>
  <overwrite>false</overwrite>

  <charset>utf8</charset>

  <table>

    <name>*dbprefix*test</name>

    <declaration>

      <field>
        <name>id</name>
        <type>integer</type>
        <default>0</default>
        <notnull>true</notnull>
        <autoincrement>1</autoincrement>
        <unsigned>true</unsigned>
        <length>4</length>
      </field>

      <field>
        <name>username</name>
        <type>varchar</type>
        <default></default>
        <notnull>true</notnull>
        <unsigned>true</unsigned>
        <length>50</length>
      </field>

      <field>
        <name>sessionid</name>
        <type>integer</type>
        <default></default>
        <notnull>true</notnull>
        <length>40</length>
      </field>

      <field>
        <name>password</name>
        <type>text</type>
        <default><default>
        <notnull>true</notnull>
        <length>255</length>
      </field>

    </declaration>

  </table>

</database>
