<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
  <name>*dbname*</name>
  <create>true</create>
  <overwrite>true</overwrite>
  <charset>utf8</charset>
  <table>
    <name>*dbprefix*fileSync</name>
    <declaration>
      <field>
        <name>id</name>
        <type>integer</type>
        <default>0</default>
        <notnull>1</notnull>
        <autoincrement>1</autoincrement>
        <length>4</length>
      </field>
      <field>
        <name>username</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>50</length>
      </field>
      <field>
        <name>sessionid</name>
        <type>integer</type>
        <notnull>true</notnull>
        <length>40</length>
      </field>
      <field>
        <name>password</name>
        <type>text</type>
        <notnull>true</notnull>
        <length>255</length>
      </field>
    </declaration>
  </table>
</database>
